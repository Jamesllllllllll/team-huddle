{
  "name": "huddle",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npx convex dev --once && concurrently -r npm:dev:web npm:dev:db",
    "dev:web": "cross-env NODE_OPTIONS=--import=./instrument.server.mjs vite dev",
    "dev:db": "convex dev --run board:seed",
    "build": "vite build && tsc --noEmit && node ./scripts/copy-instrument.mjs",
    "start": "node --import ./dist/server/instrument.server.mjs ./dist/server/index.js",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "test": "vitest run",
    "test:watch": "vitest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@clerk/backend": "^2.22.0",
    "@clerk/clerk-react": "^5.55.0",
    "@clerk/themes": "^2.4.36",
    "@convex-dev/react-query": "0.0.0-alpha.8",
    "@fontsource-variable/public-sans": "^5.2.7",
    "@mendable/firecrawl-js": "^4.5.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/tanstackstart-react": "^10.24.0",
    "@tanstack/react-query": "^5.90.0",
    "@tanstack/react-query-devtools": "^5.90.0",
    "@tanstack/react-router": "^1.134.18",
    "@tanstack/react-router-devtools": "^1.134.18",
    "@tanstack/react-router-ssr-query": "^1.134.18",
    "@tanstack/react-start": "^1.134.18",
    "@useautumn/convex": "^0.0.15",
    "autumn-js": "^0.1.47",
    "aws4fetch": "^1.0.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "convex": "^1.19.0",
    "ky": "^1.7.4",
    "lucide-react": "^0.553.0",
    "motion": "^12.23.24",
    "msw": "^2.7.0",
    "openai": "^6.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.1",
    "redaxios": "^0.5.1",
    "tailwind-merge": "^2.6.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.14.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
